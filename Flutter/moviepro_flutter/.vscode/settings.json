{
    //配置新建文件注释和方法注释
    "fileheader.configObj": {
        "createFileTime": true, // 设置为true则为文件新建时候作为date，否则注释生成时间为date
        "autoAdd": true, // 自动生成注释，老是忘记的同学可以设置
        "annotationStr": {
            "head": "/*",
            "middle": " * @",
            "end": " */",
            "use": true // 设置自定义注释可用
        },
        "headInsertLine": {
            "php": 2
        },
        "language": {
            // 普通后缀文件
            "dart": {
                "head": "/**",
                "middle": " * @",
                "end": " */"
            },
        },
    },
    "fileheader.cursorMode": {
        "description": "",
        "param ": "",
        "return": ""
    },
    "fileheader.customMade": {
        "Description": "", // 文件内容描述
        "Author": "王智刚", // 编辑人
        "Date": "Do not edit", //时间
        "LastEditTime": "Do not edit",
        "LastEditors": " ",
    }
}